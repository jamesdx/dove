{
  "name": "dove-auth-ui",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "format": "prettier --write src/",
    "test:unit": "jest",
    "test:e2e": "cypress run",
    "i18n:extract": "vue-i18n-extract report -v './src/**/*.?(ts|vue)' -l './src/locales/*.json'"
  },
  "dependencies": {
    "@vueuse/core": "^10.7.0",
    "ant-design-vue": "^4.0.7",
    "axios": "^1.6.2",
    "pinia": "^2.1.7",
    "vue": "^3.3.11",
    "vue-i18n": "^9.8.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "cypress": "^13.6.1",
    "eslint": "^8.55.0",
    "eslint-plugin-vue": "^9.19.2",
    "jest": "^29.7.0",
    "less": "^4.2.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.7",
    "vue-i18n-extract": "^2.0.7",
    "vue-tsc": "^1.8.25"
  }
} 